appcenter codepush deployment list -a Fatema-Patel/AppCenter 
appcenter codepush release-react -a Fatema-Patel/AppCenter -d Production
appcenter codepush release -a Fatema-Patel/AppCenter -t ">1.1.1".
appcenter codepush release -a <ownerName>/<appName> -c <updateContentsPath> -t <targetBinaryVersion> -d <deploymentName>

[-t|--target-binary-version <version>]
[-—Å|--update-contents-path <updateContentsPath>]
[-r|--rollout <rolloutPercentage>]
[--disable-duplicate-release-error]
[-k|--private-key-path <privateKeyPath>]
[-m|--mandatory]
[-x|--disabled]
[--description <description>]
[-d|--deployment-name <deploymentName>]
[-a|--app <ownerName>/<appName>]
[--disable-telemetry]
